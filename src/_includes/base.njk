  <body style="background: #FCFAFA">
    {% include "header.njk" %}

    <main>
      {{ content | safe }}
      {{ scripts | safe }}
    </main>
    {% include "footer.njk" %}

    <script>
      if (window.netlifyIdentity) {
        window
          .netlifyIdentity
          .on("init", user => {
            if (!user) {
              window
                .netlifyIdentity
                .on("login", () => {
                  document.location.href = "/admin/";
                });
            }
          });
      }
    </script>

